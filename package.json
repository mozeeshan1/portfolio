{
  "name": "portfolio",
  "version": "1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "pages:watch": "npx @cloudflare/next-on-pages --watch",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-date=2023-12-18 --compatibility-flag=nodejs_compat"
  },
  "dependencies": {
    "@supabase/supabase-js": "latest",
    "flubber": "^0.4.2",
    "framer-motion": "latest",
    "next": "latest",
    "next-themes": "latest",
    "react": "^18",
    "react-dom": "^18",
    "react-slick": "latest",
    "slick-carousel": "latest"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.8.2",
    "@types/node": "^20.12.13",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-slick": "^0.23.13",
    "autoprefixer": "^10.0.1",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.2.3",
    "eslint-plugin-next-on-pages": "^1.8.2",
    "postcss": "^8",
    "tailwindcss": "latest",
    "typescript": "^5.4.5",
    "vercel": "^34.2.3"
  }
}
